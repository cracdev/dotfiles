#!/bin/sh

tmux new-session -s KL -n Dev
tmux split-window -v -p 10 -t KL
tmux select-pane -t 1
tmux split-window -h -p 30 -t KL
tmux new-window -n Lab
tmux select-pane -t 1
tmux split-window -h -p 30 -t KL
tmux select-pane -t 1
tmux split-window -v -p 20 -t KL
tmux new-window -n hacking
tmux select-pane -t 1
tmux split-window -h -p 30 -t KL
tmux select-pane -t 1
tmux split-window -v -p 20 -t KL
tmux new-window -n shell
tmux select-pane -t 1
tmux split-window -h -p 50 -t KL
tmux select-window -t KL:1

