#!/bin/sh

tmux new-session -s sm -n SM
tmux split-window -v -p 10 -t sm
tmux select-pane -t 1
tmux split-window -h -p 30 -t sm
tmux new-window -n Lab
tmux select-pane -t 1
tmux split-window -h -p 30 -t sm
tmux select-pane -t 1
tmux split-window -v -p 20 -t sm
tmux new-window -n hacking
tmux select-pane -t 1
tmux split-window -h -p 30 -t sm
tmux select-pane -t 1
tmux split-window -v -p 20 -t sm
tmux new-window -n shell
tmux select-pane -t 1
tmux split-window -h -p 50 -t sm
tmux select-window -t sm:1

